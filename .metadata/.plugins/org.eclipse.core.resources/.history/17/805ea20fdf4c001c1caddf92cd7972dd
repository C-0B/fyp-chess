package help;

public class bitBoard {

	public bitBoard() {
		// TODO Auto-generated constructor stub
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int value = 0x77;
		int rank = (value >> 4) + 1;
		int file = value & 7;
		
		System.out.println(rank + ""+ file);
		
		int idk = 0b00001111;
		System.out.println(idk);
		

	}

}
